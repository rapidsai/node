{
  "private": true,
  "name": "node-rapids",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "author": "NVIDIA, Inc. (https://nvidia.com/)",
  "maintainers": [
    "Paul Taylor <paul.e.taylor@me.com>"
  ],
  "scripts": {
    "clean": "scripts/exec.js clean",
    "doc": "rimraf doc && typedoc --options typedoc.js && yarn doc:packages",
    "doc:packages": "scripts/exec.js run doc",
    "demo": "scripts/exec.js demo",
    "test": "scripts/exec.js test",
    "lint": "scripts/exec.js lint",
    "build": "scripts/exec.js run build --concurrency 1",
    "build:debug": "scripts/exec.js run build:debug --concurrency 1",
    "compile": "scripts/exec.js run compile --concurrency 1",
    "compile:debug": "scripts/exec.js run compile:debug --concurrency 1",
    "rebuild": "scripts/exec.js run rebuild --concurrency 1",
    "rebuild:debug": "scripts/exec.js run rebuild:debug --concurrency 1",
    "cpp:build": "scripts/exec.js run cpp:build --concurrency 1",
    "cpp:compile": "scripts/exec.js run cpp:compile --concurrency 1",
    "cpp:rebuild": "scripts/exec.js run cpp:rebuild --concurrency 1",
    "cpp:build:debug": "scripts/exec.js run cpp:build:debug --concurrency 1",
    "cpp:compile:debug": "scripts/exec.js run cpp:compile:debug --concurrency 1",
    "cpp:rebuild:debug": "scripts/exec.js run cpp:rebuild:debug --concurrency 1",
    "docker:build:devel": "touch .env && docker-compose build devel",
    "docker:build:runtime": "touch .env && docker-compose build runtime",
    "docker:build:nteract": "touch .env && docker-compose build nteract",
    "docker:run:devel": "touch .env && docker-compose run --rm devel",
    "docker:run:runtime": "touch .env && docker-compose run --rm runtime",
    "docker:run:nteract": "touch .env && docker-compose run --rm nteract",
    "tsc:build": "scripts/exec.js run tsc:build",
    "tsc:watch": "scripts/exec.js run tsc:watch --parallel",
    "dev:relink-bin-dirs": "scripts/exec.js relink-bin-dirs",
    "dev:install-cpp-dependencies": "modules/core/bin/exec_install_deps.js",
    "nuke:from:orbit": "yarn cache clean && yarn clean && yarn && yarn rebuild",
    "postinstall": "yarn dev:relink-bin-dirs"
  },
  "workspaces": [
    "modules/*",
    "modules/demo/graph",
    "modules/demo/luma",
    "modules/demo/spatial",
    "modules/demo/xterm",
    "modules/demo/ipc/*",
    "modules/demo/deck/*",
    "modules/demo/umap",
    "modules/demo/tfjs/*",
    "modules/demo/client-server"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "cmake-js": "6.0.0",
    "dotenv": "8.2.0",
    "eslint": "7.27.0",
    "lerna": "3.22.1",
    "lint-staged": "10.5.1",
    "node-addon-api": "^3.1.0",
    "pre-git": "3.17.1",
    "rimraf": "3.0.0",
    "typedoc": "0.20.32"
  },
  "config": {
    "pre-git": {
      "allow-untracked-files": true,
      "pre-commit": [
        "npx lint-staged --relative"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  },
  "lint-staged": {
    "modules/**/@(src|test|include)/**/*.@(ts|h|cc?|cuh?|(c|h)pp)": [
      "scripts/exec.js lint"
    ]
  },
  "resolutions": {
    "**/jsdom": "^16.2.2",
    "**/deck.gl": "8.4.17",
    "**/react-map-gl": "5.3.16",
    "**/apache-arrow": "4.0.0",
    "**/@deck.gl/arcgis": "8.4.17",
    "**/@deck.gl/aggregation-layers": "8.4.17",
    "**/@deck.gl/core": "8.4.17",
    "**/@deck.gl/extensions": "8.4.17",
    "**/@deck.gl/geo-layers": "8.4.17",
    "**/@deck.gl/google-maps": "8.4.17",
    "**/@deck.gl/json": "8.4.17",
    "**/@deck.gl/jupyter-widget": "8.4.17",
    "**/@deck.gl/layers": "8.4.17",
    "**/@deck.gl/mapbox": "8.4.17",
    "**/@deck.gl/mesh-layers": "8.4.17",
    "**/@deck.gl/react": "8.4.17",
    "**/@deck.gl/test-utils": "8.4.17",
    "**/@loaders.gl/3d-tiles": "2.3.13",
    "**/@loaders.gl/arrow": "2.3.13",
    "**/@loaders.gl/basis": "2.3.13",
    "**/@loaders.gl/compression": "2.3.13",
    "**/@loaders.gl/core": "2.3.13",
    "**/@loaders.gl/crypto": "2.3.13",
    "**/@loaders.gl/csv": "2.3.13",
